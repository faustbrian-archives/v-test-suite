        "@types/rimraf": "^3.0.0",
        "@yarnpkg/pnpify": "^2.4.0",
        "codecov": "^3.8.1",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.4.4",
